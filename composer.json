{
    "name": "italofantone/marky",
    "description": "A simple Markdown + Easy for Laravel.",
    "keywords": ["laravel", "markdown", "easy", "commonmark"],
    "type": "library",
    "require-dev": {
        "orchestra/testbench": "^10.3",
        "phpunit/phpunit": "^12.1"
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "league/commonmark": "^2.6"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Italofantone\\Marky\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Italofantone\\Marky\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Italo Morales Fantone",
            "email": "i@rimorsoft.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Italofantone\\Marky\\MarkyServiceProvider"
            ]
        }
    }
}
